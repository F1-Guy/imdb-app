@page
@model imdb_app.Pages.AddTitleModel
@{
}

<h1>Add Title</h1>

<form method="post">
    <div class="form-group">
        <label for="tconst">tconst</label>
        <input asp-for="Title.Tconst" class="form-control" readonly placeholder="tconst is automatically generated"/>
    </div>
    <div class="form-group">
        <label for="titleType">titleType</label>
        <input asp-for="Title.TitleType" class="form-control" />
    </div>
    <div class="form-group">
        <label for="primaryTitle">primaryTitle</label>
        <input asp-for="Title.PrimaryTitle" class="form-control" />
    </div>
    <div class="form-group">
        <label for="originalTitle">originalTitle</label>
        <input asp-for="Title.OriginalTitle" class="form-control" />
    </div>
    <div class="form-group">
        <label for="startYear">startYear</label>
        <input asp-for="Title.StartYear" class="form-control" />
    </div>
    <div class="form-group">
        <label for="endYear">endYear</label>
        <input asp-for="Title.EndYear" class="form-control" />
    </div>
    <div class="form-group">
        <label for="runtimeMinutes">runtimeMinutes</label>
        <input asp-for="Title.RuntimeMinutes" class="form-control" />
    </div>
    <div class="form-group">
        <label for="genres">Genres</label>
        <input asp-for="Genres" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary my-2">Add Title</button>
</form>